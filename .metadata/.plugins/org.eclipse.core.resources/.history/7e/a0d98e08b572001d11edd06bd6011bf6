public class VeggieLoversPizza extends Pizza {
//Constructor for the VeggieLoversPizza class
public VeggieLoversPizza() {
//Set the size and type of the pizza
size = Size.LARGE;
type = "veggie";
//Set the toppings of the pizza
String[] toppings = {"mushroom", "onion", "pepper"};
setToppings(toppings);
}
}